{
  "homepage": "https://dina07.github.io/Dbuget/",
  "name": "dbudget",
  "version": "1.0.0",
  "description": "Personal Budget & Expenses Tracker - A lightweight web app for tracking budgets and expenses",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "deploy": "gh-pages -d dist",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "test": "vitest"
  },
  "dependencies": {
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^3.5.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  },
  "keywords": [
    "budget",
    "expenses",
    "tracker",
    "personal-finance",
    "react",
    "tailwind",
    "typescript"
  ],
  "author": "",
